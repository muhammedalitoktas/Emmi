#include<stdio.h>
int main()
{
    int gorkemin,gotu,ve,basi,top=0;
    printf("gorkemin ");
    scanf("%d",&gorkemin);
    printf("gotu ");
    scanf("%d",&gotu);
    printf("ve ");
    scanf("%d",&ve);
    printf("basi ");
    scanf("%d",&basi);
    top=gorkemin+gotu+ve+basi;
    printf("sonuc %d",top);
	return 0;
}
